<script>
    // Скрыть содержимое экспериментов при загрузке и сохранить исходные подписи кнопок
    document.addEventListener('DOMContentLoaded', function () {
        document.querySelectorAll('.main-anomaly .experiment .content').forEach(function (c) {
            c.style.display = 'none';
        });

        document.querySelectorAll('.main-anomaly .experiment button').forEach(function (btn) {
            // сохраняем оригинальный текст в data-original-text, если ещё не задано
            if (!btn.dataset.originalText) {
                btn.dataset.originalText = btn.textContent.trim();
            }
        });
    });

    function toggleExperiment(button) {
        const container = button.closest('.experiment');
        if (!container) return;

        const content = container.querySelector('.content');
        if (!content) return;

        // если ещё не сохраняли оригинальную подпись — сохраним
        if (!button.dataset.originalText) {
            button.dataset.originalText = button.textContent.trim();
        }

        const isHidden = (content.style.display === "none" || content.style.display === "");
        if (isHidden) {
            content.style.display = "block";
            button.textContent = "Скрыть";
        } else {
            content.style.display = "none";
            // восстанавливаем ту подпись, которая была изначально
            button.textContent = button.dataset.originalText;
        }
    }
</script>